
{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)vercel.app/(.*)",
      "destination": "https://www.namoindianamkeen.com/$2",
      "permanent": true
    }
  ]
}
